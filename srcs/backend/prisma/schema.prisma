// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
	id	Int @id
	email	String?	@unique
	login	String?	@unique
	first_name	String?
	last_name	String?
	url	String?
	displayname	String?
	image_url	String?
	oauth_id	Int @unique
	oauth Oauth? @relation(fields: [oauth_id], references: [id])
}

model Tech {
	id Int @default(autoincrement()) @id
	name String @unique
	category String?
	details String?
}


model Oauth {
	id Int @default(autoincrement()) @id
	code String @unique
	refresh_token String? @unique
	access_token String? @unique
	user User?
}
